<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yhz.service">

    <permission android:name="com.yhz.service.USE_AIDL" />
    <permission android:name="com.yhz.service.USE_MESSENGER" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".AIDLService"
            android:permission="com.yhz.service.USE_AIDL">
            <intent-filter>
                <action android:name="com.yhz.service.AIDLService" />
            </intent-filter>
        </service>
        <service
            android:name=".MessengerService"
            android:permission="com.yhz.service.USE_MESSENGER">
            <intent-filter>
                <action android:name="com.yhz.service.MessengerService" />
            </intent-filter>
        </service>
    </application>

</manifest>